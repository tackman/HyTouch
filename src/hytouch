#!/usr/bin/hy

(import subprocess)
(import sys)

(print sys.argv)




(defn hytouch [package-dict]
  package-dict)

(defn hytouch-tasks []
  (.get package.package "tasks" ))

(defn hytouch-dependencies []
  (.get package.package "dependencies" ))

(defn hytouch-run [task]
  (subprocess.run (.get (hytouch-tasks) task)))

(import package)
(print package.package)

(hytouch-run "test")
